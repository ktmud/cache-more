{
  "name": "setup-superset-action",
  "version": "1.0.0",
  "private": true,
  "main": "dist/run",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "clean": "rm -rf ./lib ./dist",
    "lint": "eslint src/**/*.ts",
    "build": "npm run clean && tsc && ncc build -o dist src/run.ts && ncc build -o dist/scripts/cache src/scripts/cache.ts",
    "test": "jest --clearCache && jest --coverage",
    "all": "npm run format && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "superset"
  ],
  "dependencies": {
    "@actions/cache": "actions/cache#78809b91d73d12e094c66654de7b65727733dcc6",
    "@actions/core": "1.2.3",
    "@actions/exec": "1.0.3",
    "@actions/setup-python": "actions/setup-python#4ff1108c39d694f52d6bc366cc6a9e0b6cec890c",
    "@types/uuid": "7.0.2",
    "hasha": "5.2.0"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "12.12.35",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "@zeit/ncc": "0.22.1",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.8.2",
    "jest": "25.3.0",
    "js-yaml": "3.13.1",
    "prettier": "2.0.4",
    "ts-jest": "25.3.1",
    "typescript": "3.8.3"
  }
}
